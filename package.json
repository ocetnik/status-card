{
  "name": "status-card",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://ocetnik.github.io/status-card",
  "dependencies": {
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0"
  },
  "config": {
    "prettier": "**/*.{css,js,json,jsx}"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "prettier": "prettier --list-different $npm_package_config_prettier",
    "prettier:fix": "prettier --write $npm_package_config_prettier"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
